


<div id='chart'></div>
<%= render_chart @chart, 'chart' %>




















<h1>Listing geos</h1>

<table>
  <tr>
    <th>Country</th>
    <th>Popularity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @geos.each do |geo| %>
  <tr>
    <td><%= geo.country %></td>
    <td><%= geo.popularity %></td>
    <td><%= link_to 'Show', geo %></td>
    <td><%= link_to 'Edit', edit_geo_path(geo) %></td>
    <td><%= link_to 'Destroy', geo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Geo', new_geo_path %>
